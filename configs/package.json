{
  "name": "eslint-config-xx",
  "version": "2.0.0-beta.2",
  "description": "eslint config xx",
  "main": "eslint.config.mjs",
  "module": "eslint.config.mjs",
  "keywords": [
    "eslint",
    "eslint-config",
    "prettier",
    "prettier config"
  ],
  "author": "ShineShao <xiaoshaoqq@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .js,.mjs,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.mjs,.jsx,.ts,.tsx --fix",
    "fmt": "prettier --write \"**/*.{js,mjs,jsx,ts,tsx,css,md,json}\"",
    "fmt:check": "prettier --check \"**/*.{js,mjs,jsx,ts,tsx,css,md,json}\"",
    "lint:fmt": "npm run lint && npm run fmt:check"
  },
  "files": [
    "eslint.config.mjs",
    "prettier.config.mjs",
    "tsconfig.json"
  ],
  "dependencies": {
    "@eslint/js": "^9.33.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "eslint-config-love": "^121.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "repository": "git@github.com:freeshineit/eslint-config-xx.git",
  "bugs": {
    "url": "https://github.com/freeshineit/eslint-config-xx/issues"
  },
  "homepage": "https://github.com/freeshineit/eslint-config-xx",
  "engines": {
    "node": ">=18"
  }
}
